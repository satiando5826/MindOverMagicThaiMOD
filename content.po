msgstr ""

#. Key: KeywordDefinition.Priority_Emergency.Description
msgctxt "KeywordDefinition.Priority_Emergency.Description"
msgid "This job will be prioritized over everything besides other Emergency Priority jobs, including Eating and Sleeping. Mages will be roused from their normal Sleep to perform these jobs."
msgstr ""

#. Key: KeywordDefinition.Priority_Top.DisplayName
msgctxt "KeywordDefinition.Priority_Top.DisplayName"
